  model: @contact,
  update_path: account_contact_path(current_user.account, @contact)
   %>
<% end %>
<%= render 'components/overlays/drawer', title: t('activerecord.models.contact.edit'), width_class: 'w-2/5' do %>
  <%= render 'form', contact: @contact %>
<% end %>
